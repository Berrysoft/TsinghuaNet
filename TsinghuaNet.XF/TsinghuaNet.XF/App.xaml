<?xml version="1.0" encoding="utf-8" ?>
<Application 
    x:Class="TsinghuaNet.XF.App"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:dg="clr-namespace:Xamarin.Forms.DataGrid;assembly=Xamarin.Forms.DataGrid"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Application.Resources>
        <Color x:Key="SystemAccentColor">#FF0078D7</Color>
        <Color x:Key="SystemAccentColorDark1">#FF005A9E</Color>
        <Color x:Key="SystemAccentColorDark2">#FF004275</Color>
        <dg:PaletteCollection x:Key="DataGridForegroundPalette">
            <Color>#FF000000</Color>
        </dg:PaletteCollection>
        <Color x:Key="PopupBackground">#FFFFFFFF</Color>
        <OnPlatform x:Key="SymbolFont" x:TypeArguments="x:String">
            <On Platform="UWP,iOS">Segoe MDL2 Assets</On>
            <On Platform="Android">segmdl2.ttf#Segoe MDL2 Assets</On>
        </OnPlatform>
    </Application.Resources>
</Application>