<Application
    x:Class="TsinghuaNet.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TsinghuaNet.UWP">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls"/>
                <ResourceDictionary>
                    <CubicEase x:Key="ShowAnimationEase" EasingMode="EaseOut"/>
                    <CubicEase x:Key="HideAnimationEase" EasingMode="EaseIn"/>
                    <local:EnumIndexConverter x:Key="EnumIndexConverter"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
