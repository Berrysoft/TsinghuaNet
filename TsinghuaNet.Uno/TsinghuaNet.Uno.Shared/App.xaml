<Application
    x:Class="TsinghuaNet.Uno.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TsinghuaNet.Uno"
    xmlns:converters="using:TsinghuaNet.Uno.Converters">
    <Application.Resources>
        <CubicEase x:Key="ShowAnimationEase" EasingMode="EaseOut"/>
        <CubicEase x:Key="HideAnimationEase" EasingMode="EaseIn"/>
        <converters:EnumIndexConverter x:Key="EnumIndexConverter"/>
    </Application.Resources>
</Application>
